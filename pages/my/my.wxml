<view class="user">
  <!--user-info-->
  <view class="user_info">
    <image class="user_avatural" src="{{ userInfo.avatarUrl }}"></image>
    <text class="user_name">{{ userInfo.nickName }}</text>
    <text class="jfye">积分余额：{{ 9999 }}</text>
  </view>
  <!--user-list-->
  <view class="user_list">
    <navigator url="" hover-class="none" class="triangle {{ item.icon }}" wx:for="{{ userList }}" wx:key="unique">  {{ item.title }}</navigator>
  </view>
</view>